{
  "extends": "@tsconfig/node14/tsconfig.json",
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "es2022",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "outDir": "./dist",
    "skipLibCheck": false,
    "strict": true,
    "target": "ES2021",
    "noUncheckedIndexedAccess": true
  }
}
